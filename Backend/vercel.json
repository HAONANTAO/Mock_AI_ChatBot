{
  "functions": {
    "dist/routes/user-routes.js": {
      "runtime": "nodejs20.x"
    },
    "dist/routes/chat-routes.js": {
      "runtime": "nodejs20.x"
    }
  },
  "routes": [
    { "src": "/api/v1/user/(.*)", "dest": "/dist/routes/user-routes.js" },
    { "src": "/api/v1/chat/(.*)", "dest": "/dist/routes/chat-routes.js" }
  ]
}
