{
  "version": 2,
  "builds": [
    {
      "src": "deploy.sh",
      "use": "@vercel/exec-sh",
      "config": {
        "maxFiles": 10000
      }
    }
  ],
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "/api/v1/$1",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    },
    {
      "src": "/(.*)",
      "dest": "/Frontend/dist/$1",
      "methods": ["GET"]
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
