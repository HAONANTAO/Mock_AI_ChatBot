{
  "rewrites": [
    {
      "source": "/api/v1/user/(.*)",
      "destination": "/api/v1/user/$1"
    },
    {
      "source": "/api/v1/chat/(.*)",
      "destination": "/api/v1/chat/$1"
    }
  ],
  "functions": {
    "api/src/routes/chat-routes.js": {
      "runtime": "@vercel/node@5.0.2"
    },
    "api/src/routes/user-routes.js": {
      "runtime": "@vercel/node@5.0.2"
    }
  }
}
